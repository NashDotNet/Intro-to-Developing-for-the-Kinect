<Window x:Class="Kinect.Demo.DepthData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid Background="Black">
        <Image x:Name="BackgroundImage" Source="\Resources\paris-destination.jpg" Stretch="UniformToFill" HorizontalAlignment="Center" VerticalAlignment="Center" Opacity="1" Visibility="Visible" />
        <Image x:Name="DepthImage" Stretch="UniformToFill" HorizontalAlignment="Center" VerticalAlignment="Center" Opacity="1" Visibility="Visible" />
        <Image x:Name="GodzillaImage" Source="\Resources\Godzilla.gif" Stretch="UniformToFill" HorizontalAlignment="Center" VerticalAlignment="Center" Opacity="1" Visibility="Visible" />
    </Grid>
</Window>
